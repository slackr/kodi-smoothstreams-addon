						<top>0</top>
						<width>100%</width>
						<height>100%</height>
						<aspectratio>scale</aspectratio>
						<texture colordiffuse="Window(10000).Property(script.smoothstreams-v3.colour.background)">backgrounds/primary.jpg</texture>
					</control>
					<control type="image">
						<depth>-0.80</depth>
						<left>0</left>
						<top>0</top>
						<width>100%</width>
						<height>100%</height>
